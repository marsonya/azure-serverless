{
  "name": "@nestjs/azure-serverless",
  "version": "0.0.5",
  "description": "Nest - modern, fast, powerful node.js web framework (@azure-serverless)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "postbuild": "copyfiles -u 1 lib/**/*.json dist",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "devDependencies": {
    "@nestjs/azure-func-http": "0.3.1",
    "@nestjs/common": "6.10.13",
    "@nestjs/core": "6.10.13",
    "@nestjs/platform-express": "6.10.13",
    "@types/memory-fs": "0.3.2",
    "@types/node": "7.10.7",
    "@types/webpack": "4.41.0",
    "copyfiles": "2.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@nestjs/serverless-core": "0.2.1",
    "memory-fs": "0.5.0",
    "ts-morph": "5.0.0",
    "webpack": "4.41.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
